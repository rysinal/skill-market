{
  "name": "skill-market",
  "version": "0.1.0",
  "description": "CLI tool for managing AI skills from private registries",
  "type": "module",
  "bin": {
    "skill-market": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "ora": "^8.0.0",
    "tar": "^7.0.0",
    "undici": "^6.6.0",
    "@skill-market/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^20.11.0",
    "@types/tar": "^6.1.0",
    "@vitest/coverage-v8": "^2.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cli",
    "skills",
    "ai",
    "claude",
    "codex"
  ],
  "author": "",
